@model Blog.Web.Models.Gallery.PhotoGalleryViewModel

@{
    ViewData["Title"] = "Фотогалерея";
}

@section Stylesheets {
    <environment include="Development">
        <link rel="stylesheet" href="~/css/photo-gallery/_gallery-item-partial.css" />
        <link rel="stylesheet" href="~/css/photo-gallery/_gallery-partial.css" />
        <link rel="stylesheet" href="~/css/photo-gallery/index.css" />
        <link rel="stylesheet" href="~/css/shared/_pager-partial.css" />
    </environment>
    <environment exclude="Development">
        <link rel="stylesheet" href="~/css/photo-gallery/index.min.css" />
    </environment>
}

@await Html.PartialAsync("_GalleryPartial", Model.GalleryItems)
@await Html.PartialAsync("_PagerPartial", Model)